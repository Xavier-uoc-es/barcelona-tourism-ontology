###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:comment "A site of tourist interest within a city." ;
            rdfs:label "Attraction" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#BudgetHotel
:BudgetHotel rdf:type owl:Class ;
             rdfs:subClassOf :Hotel ;
             rdfs:comment "A low-cost hotel providing basic services." ;
             rdfs:label "Budget Hotel" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "A large motor vehicle carrying passengers by road." ;
     rdfs:label "Bus" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#CapitalCity
:CapitalCity rdf:type owl:Class ;
             rdfs:subClassOf :City ;
             rdfs:comment "A city that serves as the governmental center of a country." ;
             rdfs:label "Capital City" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#City
:City rdf:type owl:Class ;
      rdfs:comment "A geographical area where tourist activities take place." ;
      rdfs:label "City" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#CoastalCity
:CoastalCity rdf:type owl:Class ;
             rdfs:subClassOf :City ;
             rdfs:comment "A city located near or along a coastline." ;
             rdfs:label "Coastal City" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Concert
:Concert rdf:type owl:Class ;
         rdfs:subClassOf :Event ;
         rdfs:comment "A musical performance typically held in a public venue." ;
         rdfs:label "Concert" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Event
:Event rdf:type owl:Class ;
       rdfs:comment "A public or social gathering happening in a city." ;
       rdfs:label "Event" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Exhibition
:Exhibition rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "A public display of works of art or other items of interest." ;
            rdfs:label "Exhibition" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#ExpensiveRestaurant
:ExpensiveRestaurant rdf:type owl:Class ;
                     rdfs:subClassOf :Restaurant ;
                     rdfs:comment "A restaurant classified as expensive due to its high average price." ;
                     rdfs:label "Expensive Restaurant" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#FastFoodRestaurant
:FastFoodRestaurant rdf:type owl:Class ;
                    rdfs:subClassOf :Restaurant ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :averagePrice ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:float
                                    ] ;
                    rdfs:comment "A restaurant offering quick and affordable meals." ;
                    rdfs:label "Fast Food Restaurant" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Festival
:Festival rdf:type owl:Class ;
          rdfs:subClassOf :Event ;
          rdfs:comment "A day or period of celebration, often involving cultural activities." ;
          rdfs:label "Festival" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#GourmetRestaurant
:GourmetRestaurant rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
                   rdfs:comment "A restaurant specializing in high-quality cuisine." ;
                   rdfs:label "Gourmet Restaurant" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:comment "A place offering accommodation to tourists." ;
       rdfs:label "Hotel" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#LuxuryHotel
:LuxuryHotel rdf:type owl:Class ;
             rdfs:subClassOf :Hotel ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :rating ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:float
                             ] ;
             rdfs:comment "A high-end hotel offering premium services and facilities." ;
             rdfs:label "Luxury Hotel" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :Attraction ;
          rdfs:comment "A structure built to commemorate a person or event." ;
          rdfs:label "Monument" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :Attraction ;
        rdfs:comment "An institution that preserves and exhibits objects of historical or artistic value." ;
        rdfs:label "Museum" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :Attraction ;
      rdfs:comment "A public green area for recreation in a city." ;
      rdfs:label "Park" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Plane
:Plane rdf:type owl:Class ;
       rdfs:subClassOf :Transport ;
       rdfs:comment "A vehicle designed for air travel." ;
       rdfs:label "Plane" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#PremiumHotel
:PremiumHotel rdf:type owl:Class ;
              rdfs:subClassOf :Hotel ;
              rdfs:comment "A hotel classified as premium due to its high rating." ;
              rdfs:label "PremiumHotel" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#PublicTransport
:PublicTransport rdf:type owl:Class ;
                 rdfs:subClassOf :Transport ;
                 rdfs:comment "A means of transport available for public use, typically including buses, trains, and similar vehicles." ;
                 rdfs:label "Public Transport" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:comment "A place where tourists can eat meals." ;
            rdfs:label "Restaurant" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Train
:Train rdf:type owl:Class ;
       rdfs:subClassOf :Transport ;
       rdfs:comment "A railway vehicle that transports people or goods." ;
       rdfs:label "Train" .


###  http://www.semanticweb.org/xavier/ontologies/2025/3/untitled-ontology-5#Transport
:Transport rdf:type owl:Class ;
           rdfs:comment "A system or means of traveling from one place to another." ;
           rdfs:label "Transport" .


